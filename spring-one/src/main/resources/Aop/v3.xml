<?xml version="1.0" encoding="UTF-8"?>
<beans
        xmlns="http://www.springframework.org/schema/beans"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:p="http://www.springframework.org/schema/p"
        xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">

    <bean id="cartridge" class="com.hcq.xxx.one.v3.Cartridge" p:color="红色"/>
    <bean id="paper" class="com.hcq.xxx.one.v3.Paper" p:paperprice="45" p:papertyoe="A4"/>
    <bean id="printerUse" class="com.hcq.xxx.one.v3.PrinterUse">
        <property name="paper" ref="paper"/>
        <property name="cartridge" ref="cartridge"/>
    </bean>
</beans>